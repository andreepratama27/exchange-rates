{
  "presets": ["react", "env"],
  "plugins": [
    ["transform-object-rest-spread"],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@assets": "./src/assets",
          "@components": "./src/components",
          "@src": "./src",
          "@scenes": "./src/scenes",
          "@services": "./src/services",
          "@styles": "./src/styles"
        }
      }
    ]
  ]
}
